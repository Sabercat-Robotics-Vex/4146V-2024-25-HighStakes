   {\n      Brain.Screen.print(\"-\");\n    }\n    \n    // extra space\n    Brain.Screen.print(\" \");\n\n    if (Controller1.ButtonL1.pressing())\n    {\n      Brain.Screen.print(\"L1\");\n    }\n    else\n    {\n      Brain.Screen.print(\"--\");\n    }\n\n    if (Controller1.ButtonL2.pressing())\n    {\n      Brain.Screen.print(\"L2\");\n    }\n    else\n    {\n      Brain.Screen.print(\"--\");\n    }\n    \n    if (Controller1.ButtonR1.pressing())\n    {\n      Brain.Screen.print(\"R1\");\n    }\n    else\n    {\n      Brain.Screen.print(\"--\");\n    }\n    \n    if (Controller1.ButtonR2.pressing())\n    {\n      Brain.Screen.print(\"R2\");\n    }\n    else\n    {\n      Brain.Screen.print(\"--\");\n    }\n\n    // Controller 1 stick\n    Brain.Screen.print(\" %4d %4d %4d %4d\", Controller1.Axis1.position(), Controller1.Axis2.position(), Controller1.Axis3.position(), Controller1.Axis4.position());\n\n    Brain.Screen.setCursor(2,1);\n\n    // Controller 2 Buttons\n    if (Controller2.ButtonA.pressing())\n    {\n      Brain.Screen.print(\"A\");\n    }\n    else\n    {\n      Brain.Screen.print(\"-\");\n    }\n\n    if (Controller2.ButtonB.pressing())\n    {\n      Brain.Screen.print(\"B\");\n    }\n    else\n    {\n      Brain.Screen.print(\"-\");\n    }\n\n    if (Controller2.ButtonX.pressing())\n    {\n      Brain.Screen.print(\"X\");\n    }\n    else\n    {\n      Brain.Screen.print(\"-\");\n    }\n\n    if (Controller2.ButtonY.pressing())\n    {\n      Brain.Screen.print(\"Y\");\n    }\n    else\n    {\n      Brain.Screen.print(\"-\");\n    }\n\n    if (Controller2.ButtonUp.pressing())\n    {\n      Brain.Screen.print(\"U\");\n    }\n    else\n    {\n      Brain.Screen.print(\"-\");\n    }\n\n    if (Controller2.ButtonDown.pressing())\n    {\n      Brain.Screen.print(\"D\");\n    }\n    else\n    {\n      Brain.Screen.print(\"-\");\n    }\n\n    if (Controller2.ButtonLeft.pressing())\n    {\n      Brain.Screen.print(\"L\");\n    }\n    else\n    {\n      Brain.Screen.print(\"-\");\n    }\n\n    if (Controller2.ButtonRight.pressing())\n    {\n      Brain.Screen.print(\"R\");\n    }\n    else\n    {\n      Brain.Screen.print(\"-\");\n    }\n\n    // extra space\n    Brain.Screen.print(\" \");\n\n    if (Controller2.ButtonL1.pressing())\n    {\n      Brain.Screen.print(\"L1\");\n    }\n    else\n    {\n      Brain.Screen.print(\"--\");\n    }\n\n    if (Controller2.ButtonL2.pressing())\n    {\n      Brain.Screen.print(\"L2\");\n    }\n    else\n    {\n      Brain.Screen.print(\"--\");\n    }\n\n    if (Controller2.ButtonR1.pressing())\n    {\n      Brain.Screen.print(\"R1\");\n    }\n    else\n    {\n      Brain.Screen.print(\"--\");\n    }\n\n    if (Controller2.ButtonR2.pressing())\n    {\n      Brain.Screen.print(\"R2\");\n    }\n    else\n    {\n      Brain.Screen.print(\"--\");\n    }\n\n    // Controller 2 stick\n    Brain.Screen.print(\" %4d %4d %4d %4d\", Controller2.Axis1.position(), Controller2.Axis2.position(), Controller2.Axis3.position(), Controller2.Axis4.position());\n\n    wait(20, msec);\n  }\n}\n\nint main()\n{\n  // Initializing Robot Configuration. DO NOT REMOVE!\n  vexcodeInit();\n  // Begin project code\n  pre_auton();\n  Competition.drivercontrol(driver);\n}\n","textLanguage":"cpp","robotConfig":[{"port":[],"name":"Controller1","customName":false,"deviceType":"Controller","deviceClass":"controller","setting":{"left":"","leftDir":"false","right":"","rightDir":"false","upDown":"","upDownDir":"false","xB":"","xBDir":"false","drive":"none","id":"primary"}},{"port":[],"name":"Controller2","customName":false,"deviceType":"Controller","deviceClass":"controller","setting":{"left":"","leftDir":"false","right":"","rightDir":"false","upDown":"","upDownDir":"false","xB":"","xBDir":"false","drive":"none","id":"partner"}}],"slot":0,"platform":"V5","sdkVersion":"20240802.15.00.00","appVersion":"4.0.7","fileFormat":"2.0.0","targetBrainGen":"First","v5Sounds":[{"name":"game over","url":"static/sounds/mixkit-arcade-r