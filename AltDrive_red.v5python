{"mode":"Text","hardwareTarget":"brain","textContent":"#region VEXcode Generated Robot Configuration\nfrom vex import *\nimport urandom\n\n# Brain should be defined by default\nbrain=Brain()\n\n# Robot configuration code\ncontroller_1 = Controller(PRIMARY)\nmotor_drive_br = Motor(Ports.PORT20, GearSetting.RATIO_18_1, True)\nmotor_drive_bl = Motor(Ports.PORT16, GearSetting.RATIO_18_1, False)\nmotor_drive_fr = Motor(Ports.PORT10, GearSetting.RATIO_18_1, True)\nmotor_drive_fl = Motor(Ports.PORT15, GearSetting.RATIO_18_1, False)\nmotor_conveyer = Motor(Ports.PORT18, GearSetting.RATIO_18_1, True)\npneu_goal_catch = DigitalOut(brain.three_wire_port.a)\npneu_intake = DigitalOut(brain.three_wire_port.b)\nmotor_intake = Motor(Ports.PORT11, GearSetting.RATIO_18_1, False)\n\n\n# wait for rotation sensor to fully initialize\nwait(30, MSEC)\n\n\n# Make random actually random\ndef initializeRandomSeed():\n    wait(100, MSEC)\n    random = brain.battery.voltage(MV) + brain.battery.current(CurrentUnits.AMP) * 100 + brain.timer.system_high_res()\n    urandom.seed(int(random))\n      \n# Set random seed \ninitializeRandomSeed()\n\n\ndef play_vexcode_sound(sound_name):\n    # Helper to make playing sounds from the V5 in VEXcode easier and\n    # keeps the code cleaner by making it clear what is happening.\n    print(\"VEXPlaySound:\" + sound_name)\n    wait(5, MSEC)\n\n# add a small delay to make sure we don't print in the middle of the REPL header\nwait(200, MSEC)\n# clear the console to make sure we don't have the REPL in the console\nprint(\"\\033[2J\")\n\n#endregion VEXcode Generated Robot Configuration\n\n# ------------------------------------------\n# \n# \tProject:      VEXcode Project\n#\tAuthor:       VEX\n#\tCreated:\n#\tDescription:  VEXcode V5 Python Project\n# \n# ------------------------------------------\n\n# Library imports\nfrom vex import *\n\npneu_goal_catch.set(False)\npneu_goal_catch.set(False)\nmotor_drive_fr.set_velocity(100, PERCENT)\nmotor_drive_fl.set_velocity(100, PERCENT)\nmotor_drive_br.set_velocity(100, PERCENT)\nmotor_drive_bl.set_velocity(100, PERCENT)\nmotor_conveyer.set_velocity(90, PERCENT)\n\n# Begin project code\ndef driveFor(dist, direction):\n    motor_drive_fr.spin_for(direction, dist, TURNS, wait=False)\n    motor_drive_fl.spin_for(direction, dist, TURNS, wait=False)\n    motor_drive_br.spin_for(direction, dist, TURNS, wait=False)\n    motor_drive_bl.spin_for(direction, dist, TURNS, wait=True)\n\ndef auton():\n    motor_intake.spin(FORWARD)\n    driveFor(2.4, REVERSE)\n    pneu_goal_catch.set(True)\n    motor_conveyer.spin_for(FORWARD, 4, TURNS, wait=True)\n\n    motor_drive_fr.spin_for(FORWARD, 1.45, TURNS, wait=False)\n    motor_drive_fl.spin_for(REVERSE, 1.45, TURNS, wait=False)\n    motor_drive_br.spin_for(FORWARD, 1.45, TURNS, wait=False)\n    motor_drive_bl.spin_for(REVERSE, 1.45, TURNS, wait=True)\n    wait(0.25,SECONDS)\n\n    driveFor(2.2, FORWARD)\n    motor_conveyer.spin_for(FORWARD, 6, TURNS, wait=True)\n    # second goal\n    # 1 more ring\n    driveFor(1.3, REVERSE)\n    pneu_goal_catch.set(False)\n    driveFor(1.3, FORWARD)\n    wait(0.25,SECONDS)\n\n    motor_drive_fr.spin_for(REVERSE, 1.125, TURNS, wait=False)\n    motor_drive_fl.spin_for(FORWARD, 1.125, TURNS, wait=False)\n    motor_drive_br.spin_for(REVERSE, 1.125, TURNS, wait=False)\n    motor_drive_bl.spin_for(FORWARD, 1.125, TURNS, wait=True)\n    wait(0.25,SECONDS)\n\n    driveFor(1.45, REVERSE)\n    pneu_goal_catch.set(True)\n    wait(0.25,SECONDS)\n\n    driveFor(3.5, FORWARD)\n    wait(0.25,SECONDS)\n\n    motor_conveyer.spin_for(FORWARD, 6, TURNS, wait=True)\n    pneu_goal_catch.set(False)\n    wait(0.25, SECONDS)\n\n    driveFor(1.3, REVERSE)\n    wait(0.25, SECONDS)\n\n    motor_drive_fr.spin_for(REVERSE, 1.1257, TURNS, wait=False)\n    motor_drive_fl.spin_for(FORWARD, 1.1257, TURNS, wait=False)\n    motor_drive_br.spin_for(REVERSE, 1.1257, TURNS, wait=False)\n    motor_drive_bl.spin_for(FORWARD, 1.1257, TURNS, wait=True)\n    wait(0.25,SECONDS)\n\n    driveFor(4, FORWARD)\n    motor_drive_fr.spin_for(REVERSE, 1, TURNS, wait=False)\n    motor_drive_fl.spin_for(FORWARD, 1, TURNS, wait=False)\n    motor_drive_br.spin_for(REVERSE, 1, TURNS, wait=False)\n    motor_drive_bl.spin_for(FORWARD, 1, TURNS, wait=True)\n    wait(0.25,SECONDS)\n\n    driveFor(0.5, FORWARD)\ndef driver_control():\n    while True:\n        Hpos = controller_1.axis1.position()\n        Vpos = controller_1.axis3.position()\n\n        motor_conveyer.set_velocity(90, PERCENT)\n        motor_intake.set_velocity(100, PERCENT)\n        motor_drive_fl.set_velocity(Vpos + Hpos,PERCENT)\n        motor_drive_fr.set_velocity(Vpos - Hpos,PERCENT)\n        motor_drive_bl.set_velocity(Vpos + Hpos,PERCENT)\n        motor_drive_br.set_velocity(Vpos - Hpos,PERCENT)\n        \n         \n        motor_drive_fl.spin(FORWARD)\n        motor_drive_fr.spin(FORWARD)\n        motor_drive_bl.spin(FORWARD)\n        motor_drive_br.spin(FORWARD)\n\n        if controller_1.buttonL2.pressing() and not controller_1.buttonL1.pressing():\n            motor_intake.spin(FORWARD)\n            motor_conveyer.spin(FORWARD)\n        elif controller_1.buttonL1.pressing() and not controller_1.buttonL2.pressing():\n            motor_conveyer.spin(REVERSE)\n        elif not controller_1.buttonL2.pressing() and not controller_1.buttonL1.pressing():\n            motor_conveyer.stop()\n            motor_intake.stop()\n\n        if controller_1.buttonR2.pressing() and not controller_1.buttonR1.pressing():\n            pneu_goal_catch.set(True)\n        elif controller_1.buttonR1.pressing() and not controller_1.buttonR2.pressing():\n            pneu_goal_catch.set(False)\n\n        \n\n        wait(0.02,SECONDS)\n\ncompetition = Competition(driver_control, auton)","textLanguage":"python","robotConfig":[{"port":[],"name":"controller_1","customName":false,"deviceType":"Controller","deviceClass":"controller","setting":{"left":"","leftDir":"false","right":"","rightDir":"false","upDown":"","upDownDir":"false","xB":"","xBDir":"false","drive":"none","id":"primary"},"triportSourcePort":22},{"port":[20],"name":"motor_drive_br","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"true","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[16],"name":"motor_drive_bl","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[10],"name":"motor_drive_fr","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"true","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[15],"name":"motor_drive_fl","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1","id":"partner"},"triportSourcePort":22},{"port":[18],"name":"motor_conveyer","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"true","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[1],"name":"pneu_goal_catch","customName":true,"deviceType":"DigitalOut","deviceClass":"digital_out","setting":{"id":"partner"},"triportSourcePort":22},{"port":[2],"name":"pneu_intake","customName":true,"deviceType":"DigitalOut","deviceClass":"digital_out","setting":{"id":"partner"},"triportSourcePort":22},{"port":[11],"name":"motor_intake","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22}],"slot":1,"platform":"V5","sdkVersion":"20240802.15.00.00","appVersion":"4.0.8","fileFormat":"2.0.0","targetBrainGen":"First","v5Sounds":[{"name":"game over","url":"static/sounds/mixkit-arcade-retro-game-over-213.wav"}],"v5SoundsEnabled":false}